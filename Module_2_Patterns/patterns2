import java.util.Scanner;

public class patterns2 {
    public static void main(String[] args) {
        Scanner scn  = new Scanner(System.in);
        int n = scn.nextInt();

        for(int row = 1; row<=n; row++){
            int nstr = n+1-row;
            for(int i =1; i<=nstr; i++){
                System.out.print("*\t");
            }
            System.out.println();
        }
    }

}