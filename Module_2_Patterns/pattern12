import java.util.*;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
int n = scn.nextInt();
     int a = 0;
     int b = 1;


        for(int row = 1; row<=n; row++){
           
            for(int i = 1; i<=row; i++){
                System.out.print(a+"\t");
                int c = a+b;
                a=b;
                b=c;
            }
            System.out.println();

        }

    }
}