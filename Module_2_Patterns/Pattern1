import java.util.*;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
int n = scn.nextInt();

       // print 1 st loop 
       for(int row = 1; row<=n; row++){
           int nstr = row;
           for(int i =1; i<=nstr; i++){
               System.out.print("*\t");
           }
           System.out.println();
       }

    }
}